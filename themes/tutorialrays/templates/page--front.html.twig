
<html>
<head>
<meta charset="utf-8">
<title></title>
</head>
<body>
	<div id="header" >
	  
		{{ page.header }}
		 <div class="heading">
			<h1> WorkShop Site </h1>
		</div>
		<div class="menu">
			{{ page.menu }}
		</div>
    </div>
	<main class="main">
 	 <div class="container">
   		 <div class="row">
         <div class="col-xs-10">
           <div id="content">
			{{ page.content }}
		</div>
        </div>
        <div class="col-xs-2">
            	{% if page.sidebar %}
		  <div class="layout-sidebar-first" role="complementary">
       			 {{ page.sidebar }}
      	  </div>
		{% endif %}
        </div>
    </div>
</div>

	</main>
	<footer class="footer">
			{{ page.footer }}
	</footer>
</body>
</html>

